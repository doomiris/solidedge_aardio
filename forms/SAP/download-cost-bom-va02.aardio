        			session.findById("wnd[0]/tbar[0]/btn[0]").press
			else 
        		set tdstr = nothing
        		on error resume next
        			set tdstr = session.findById("wnd[0]/usr/subALL:SAPLCKDI:4611/tabsREITER/tabpTERM/ssubTERM:SAPLCKDI:4614/ctxtCKI64A-KADAT")
